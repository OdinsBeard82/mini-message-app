<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <%= title %>
    </title>
    <link rel="stylesheet" href="/stylesheets/style.css">
</head>

<body>
    <h1>
        <%= title %>
    </h1>
    <ul>
        <% bookinstance_list.forEach(function(bookinstance) { %>
            <li>
                <a href="<%= bookinstance.url %>">
                    <%= bookinstance.book.title %> - <%= bookinstance.imprint %>
                </a>
            </li>
            <% }); %>
    </ul>
</body>

</html>