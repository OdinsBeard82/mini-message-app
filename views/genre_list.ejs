<!DOCTYPE html>
<html>

<head>
    <title>
        <%= title %>
    </title>
</head>

<body>
    <h1>
        <%= title %>
    </h1>
    <ul>
        <% genre_list.forEach(function(genre) { %>
            <li>
                <a href="<%= genre.url %>">
                    <%= genre.name %>
                </a>
            </li>
            <% }); %>
    </ul>
</body>

</html>